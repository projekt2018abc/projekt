@model IEnumerable<ProjektZespolowy.Models.MyModels.Rezerwacja>

@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>

<p>
    <a asp-action="Create">Rezerwacja</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Date)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Date)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.RezerwacjaId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.RezerwacjaId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.RezerwacjaId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
