@model ProjektZespolowy.Models.MyModels.Rachunek

@{
    ViewData["Title"] = "Szczegóły rachunku";
}


<div class="register">
    <section class="register-wrapper">
        <h2>Szczegóły</h2>

        <div>
            <hr />
            <dl class="dl-horizontal">
                <dt>
                    Data:
                </dt>
                <dd>
                    @Html.DisplayFor(model => model.Data)
                </dd>
                <dt>
                    Paragon:
                </dt>
                <dd>
                    @Html.DisplayFor(model => model.Paragon)
                </dd>
            </dl>
        </div>
        <div class="details-bottom-nav">
            <a asp-action="Edit" asp-route-id="@Model.RachunekId">Edytuj</a> |
            <a asp-action="Index">Wróć do listy rachunków</a>
        </div>
    </section>
</div>