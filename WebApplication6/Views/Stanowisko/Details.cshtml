@model ProjektZespolowy.Models.MyModels.Stanowisko

@{
    ViewData["Title"] = "Szczegóły stanowiska";
}

<div class="register">
    <section class="register-wrapper">
        <h2>Szczegóły</h2>

        <div>
            <hr />
            <dl class="dl-horizontal">
                <dt>
                    Typ:
                </dt>
                <dd>
                    @Html.DisplayFor(model => model.Typ)
                </dd>
            </dl>
        </div>
        <br />
        <div class="details-bottom-nav">
            <a asp-action="Edit" asp-route-id="@Model.StanowiskoId">Edytuj</a> |
            <a asp-action="Index">Wróć do listy stanowisk</a>
        </div>
    </section>
</div>