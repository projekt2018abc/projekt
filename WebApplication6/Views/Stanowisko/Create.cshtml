@model ProjektZespolowy.Models.MyModels.Stanowisko

@{
    ViewData["Title"] = "Dodawanie stanowiska";
}

<div class="register">
    <section class="register-wrapper">
        <h2>Dodaj stanowisko</h2>
        <hr />
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="Typ" class="control-label">Typ</label>
                <input asp-for="Typ" class="form-control" />
                <span asp-validation-for="Typ" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Dodaj" class="btn btn-default" />
            </div>
        </form>

        <div>
            <a asp-action="Index">Wróć do listy stanowisk</a>
        </div>
    </section>
</div>
@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
